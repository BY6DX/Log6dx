<CascadingAuthenticationState>
    <Router AppAssembly="@typeof(Program).Assembly">
        <Found Context="routeData">
            <AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(Server.Layouts.BasicLayout)">
                <NotAuthorized>
                    <Server.Components.Exception._403 />
                </NotAuthorized>
            </AuthorizeRouteView>
        </Found>
        <NotFound >
            <LayoutView Layout="@typeof(Server.Layouts.BasicLayout)">
                <Server.Components.Exception._404 />
            </LayoutView>
        </NotFound>
    </Router>
</CascadingAuthenticationState>
<AntContainer />
